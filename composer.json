{
  "name": "uginroot/php-set",
  "description": "Php set type",
  "license": "MIT",
  "keywords": ["php", "type", "set"],
  "authors": [
    {
      "name": "Soroka Anton",
      "email": "uginroot@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Uginroot\\PhpSet\\": "./src",
      "Uginroot\\PhpSet\\Test\\": "./test",
      "Uginroot\\PhpSet\\Benchmark\\": "./benchmark"
    }
  },
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "8",
    "phpbench/phpbench": "@dev"
  },
  "scripts": {
    "test" : [
      "\"./vendor/bin/phpunit.bat\" --bootstrap \"./vendor/autoload.php\" --testdox \"./test\""
    ],
    "benchmark" : [
      "\"./vendor/bin/phpbench.bat\" run --report=aggregate --revs 10000 --warmup 2 --progress=blinken"
    ]
  }
}
