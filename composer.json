{
  "name": "uginroot/php-set",
  "description": "Php set type",
  "license": "MIT",
  "keywords": ["php", "type", "set"],
  "authors": [
    {
      "name": "Soroka Anton",
      "email": "uginroot@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Uginroot\\PhpSet\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uginroot\\PhpSet\\Test\\": "test",
      "Uginroot\\PhpSet\\Benchmark\\": "benchmark"
    }
  },
  "require": {
    "php": "^7.4"
  },
  "scripts": {
    "test" : [
      "phpunit --bootstrap \"./vendor/autoload.php\" --testdox \"./test\""
    ],
    "benchmark" : [
      "phpbench run --report=aggregate --revs 10000 --warmup 2 --progress=blinken"
    ]
  }
}
